			"esbuild"
		]
	},
	"dependencies": {
		"@lucia-auth/adapter-drizzle": "^1.1.0",
		"arctic": "^3.7.0",
		"drizzle-orm": "^0.40.0",
		"googleapis": "^150.0.1",
		"lucia": "^3.2.2",
		"postgres": "^3.4.5"
	},
	"packageManager": "pnpm@10.9.0"
